<p id="notice"><%= notice %></p>

<h1>Photobooths</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photobooths.each do |photobooth| %>
      <tr>
        <td><%= link_to 'Show', photobooth %></td>
        <td><%= link_to 'Edit', edit_photobooth_path(photobooth) %></td>
        <td><%= link_to 'Destroy', photobooth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photobooth', new_photobooth_path %>
